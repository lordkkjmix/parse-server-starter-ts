{
  "name": "parse-server-starter-ts",
  "version": "1.0.0",
  "private": true,
  "description": "the typescript parse-server starter project, with connected adpter, unit test and more",
  "dependencies": {
    "@parse/push-adapter": "^3.4.1",
    "@sendgrid/mail": "^7.7.0",
    "dotenv": "^8.6.0",
    "express": "^4.18.1",
    "graphql": "^15.8.0",
    "moment": "^2.29.3",
    "node-schedule": "^1.3.2",
    "parse": "^2.13.0",
    "parse-dashboard": "^2.2.0",
    "parse-server": "^4.10.13",
    "parse-server-api-mail-adapter": "^2.1.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "deploy": "node dist/index.js",
    "start-local": "tsc && node -r dotenv/config dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint-all": "eslint --cache ./dist/cloud && eslint --cache ./dist/index.js && eslint --cache ./spec",
    "lint-fix": "eslint --cache --fix ./dist/cloud && eslint --cache --fix ./dist/index.js && eslint --cache  --fix./spec",
    "test": "tsc && jasmine --reporter=jasmine-console-reporter",
    "coverage": "nyc jasmine",
    "prettier": "prettier --write '{cloud,spec}/{**/*,*}.js' 'index.js'",
    "watch-ts": "tsc -w",
    "postinstall": "npx tsc && rm -v -rf src"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.6",
    "@types/node": "^14.18.21",
    "@types/parse": "^2.18.16",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jasmine": "^3.99.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.3",
    "mongodb-runner": "^4.9.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.7.4"
  }
}
